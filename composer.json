{
    "name": "alexminza/payment-gateway-wc-maib-mia",
    "description": "Payment Gateway for maib MIA for WooCommerce",
    "type": "wordpress-plugin",
    "keywords": [
        "WooCommerce",
        "Moldova",
        "maib",
        "MIA",
        "QR",
        "bank",
        "payment",
        "gateway"
    ],
    "homepage": "https://github.com/alexminza/payment-gateway-wc-maib-mia",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Alexander Minza",
            "homepage": "https://www.alexminza.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/alexminza/payment-gateway-wc-maib-mia/issues",
        "forum": "https://wordpress.org/support/plugin/payment-gateway-wc-maib-mia",
        "source": "https://github.com/alexminza/payment-gateway-wc-maib-mia"
    },
    "require": {
        "php": ">=7.2.5",
        "alexminza/maib-mia-sdk": "^1.1",
        "alexminza/wc-payment-gateway-base": "^1.0",
        "monolog/monolog": "^2.7",
        "automattic/jetpack-autoloader": "^5.0"
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "automattic/jetpack-autoloader": true
        },
        "autoloader-suffix": "WC_Gateway_MAIB_MIA",
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "require-dev": {
        "woocommerce/woocommerce-sniffs": "^1.0"
    }
}
